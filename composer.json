{
  "name": "wp-packagist/wp-packagist",
  "description": "WordPress Package Management for WP-CLI",
  "homepage": "https://github.com/wp-packagist/wp-packagist",
  "type": "wp-cli-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Mehrshad Darzi",
      "email": "mehrshad198@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "WP_CLI_PACKAGIST\\": "src/"
    },
    "files": [
      "command.php"
    ]
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "wp-packagist/wp-cli-helper": ">=1.0.2",
    "ext-mbstring": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-zip": "*",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.0",
    "phpunit/phpunit": "^4.8"
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "commands": [
      "init",
      "install",
      "uninstall",
      "pack remove",
      "pack validate",
      "pack show",
      "pack update",
      "pack docs",
      "pack help",
      "pack edit",
      "pack tools",
      "run",
      "run admin"
    ]
  },
  "support": {
    "issues": "https://github.com/wp-packagist/wp-packagist/issues",
    "source": "https://github.com/wp-packagist/wp-packagist",
    "docs": "https://github.com/wp-packagist/wp-packagist/wiki"
  }
}
